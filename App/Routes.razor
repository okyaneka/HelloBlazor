<MudThemeProvider />
<MudPopoverProvider />
<MudDialogProvider />
<MudSnackbarProvider />

<Router AppAssembly="typeof(Program).Assembly">
  <Found Context="routeData">
    <Wrapper RouteData="@routeData">
      <RouteView RouteData="routeData" DefaultLayout="typeof(DefaultLayout)" />
      <FocusOnNavigate RouteData="routeData" Selector="h1" />
    </Wrapper>
  </Found>
  <NotFound>
    <h1 class="p-8 mx-auto text-center">Page Not Found</h1>
    <p class="mx-auto text-center">
      <MudLink href="/">Home</MudLink>
    </p>
  </NotFound>
</Router>
